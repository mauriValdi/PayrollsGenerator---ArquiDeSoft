<!DOCTYPE html>
<html>
	<head>
		<title>Payroll Generator</title>
		<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
	</head>
	<body>
		#foreach( $PayCheck in $Payroll )
		#if( $foo )
			<div>
				<div>
					<label>Id: </label>
					WorkInProgress
				</div>			
				<div>
					<label>Gross Pay: </label>
					$PayCheck.getGrossPay()
				</div>			
				<div>
					<label>Deductions: </label>
					$PayCheck.getDeductions()
				</div>
				<div>
					<label>Net Pay: </label>
					$PayCheck.getNetPay()
				</div>
			</div>
			<br>
		#{else}
		<div>
				<div>
					<label>Id: </label>
					WorkInProgress
				</div>			
				<div>
					<label>Gross Pay: </label>
					0
				</div>			
				<div>
					<label>Deductions: </label>
					0
				</div>
				<div>
					<label>Net Pay: </label>
					0
				</div>
			</div>
			<br>
		#end
		#end
	</body>
</html>